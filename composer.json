{
    "name": "digital-creative/nova-dashboard",
    "description": "The missing dashboard for nova.",
    "keywords": [
        "laravel",
        "nova",
        "dashboard",
        "widgets",
        "flexible"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.28.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\NovaDashboard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\NovaDashboard\\NovaDashboardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
